<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.testeadt.Home" >

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/FotoUsuario"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:onClick="FotoUsuarioOnClick"
        android:clickable="true"
        android:src="@drawable/semfoto"  />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="#13 "

        android:id="@+id/Num"
        android:layout_alignParentTop="@+id/FotoUsuario"

        android:layout_toRightOf="@+id/FotoUsuario" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Bruno Siqueira"
        android:id="@+id/nomeJogador"
        android:layout_below="@id/FotoUsuario"
        android:layout_alignTop="@+id/Num"
        android:layout_toRightOf="@+id/Num"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Mega Play Paintball"
        android:id="@+id/timeJogador"
        android:layout_toRightOf="@id/FotoUsuario"
        android:layout_below="@id/Num"
        android:layout_alignLeft="@id/Num"
     />




    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Snake"
        android:onClick="FeedOnClick"
        android:adjustViewBounds="true"
        android:maxWidth="70dp"
        android:maxHeight="70dp"
        android:scaleType="fitXY"

        android:src="@drawable/snake"
        android:layout_alignBottom="@+id/Coach"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/CornerSnake"
        android:maxWidth="70dp"
        android:maxHeight="70dp"
        android:adjustViewBounds="true"

        android:scaleType="fitXY"
        android:src="@drawable/snakecorner"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Snake"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/BackCenter"
        android:maxWidth="70dp"
        android:maxHeight="70dp"
        android:adjustViewBounds="true"

        android:scaleType="fitXY"
        android:src="@drawable/backcenter"
        android:layout_alignTop="@+id/CornerDoritos"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:adjustViewBounds="true"
        android:maxWidth="70dp"
        android:id="@+id/CornerDoritos"
        android:src="@drawable/cornerdoritos"
        android:layout_below="@+id/Snake"
        android:layout_alignLeft="@+id/Doritos"
        android:layout_alignStart="@+id/Doritos" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:maxWidth="70dp"
        android:scaleType="fitXY"

        android:id="@+id/Doritos"
        android:src="@drawable/doritos"
        android:layout_marginTop="74dp"
        android:layout_below="@+id/FotoUsuario"
        android:layout_alignRight="@+id/avaliacoes"
        android:layout_alignEnd="@+id/avaliacoes" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Coach"
        android:adjustViewBounds="true"
        android:maxWidth="70dp"
        android:scaleType="fitXY"

        android:onClick="LogoffOnClick"
        android:src="@drawable/coach"
        android:layout_alignTop="@+id/Doritos"
        android:layout_centerHorizontal="true" />





    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Gun Fight"
        android:id="@+id/gunfight"
        android:layout_below="@+id/CornerSnake"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="38dp" />




    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Velocidade"
        android:id="@+id/velocidade"
        android:layout_below="@id/gunfight"
        android:layout_alignParentLeft="true"
        />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Conhecimento de Jogo"
        android:id="@+id/conhecimento"
        android:layout_below="@id/velocidade"
        android:layout_alignParentLeft="true"  />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/notaVelocidade"
        style = "?android:attr/ratingBarStyleSmall"
        android:numStars = "5"
        android:rating   = "3"
        android:layout_toRightOf="@id/velocidade"
        android:layout_alignTop="@id/velocidade"
        android:layout_alignParentLeft="@id/velocidade"
        />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/notaGunfight"
        style = "?android:attr/ratingBarStyleSmall"
        android:numStars = "5"
        android:rating   = "3"
        android:layout_toRightOf="@id/gunfight"
        android:layout_alignTop="@id/gunfight"
        android:layout_alignParentLeft="@id/gunfight"

        />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/notaConhecimento"
        style = "?android:attr/ratingBarStyleSmall"
        android:numStars = "5"
        android:rating   = "3"
        android:layout_toRightOf="@id/conhecimento"
        android:layout_alignTop="@id/conhecimento"
        android:layout_alignParentLeft="@id/conhecimento"

        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9 Avaliações"
        android:id="@+id/avaliacoes"
        android:layout_below="@id/conhecimento"
        android:layout_alignParentRight="true"/>
</RelativeLayout>
